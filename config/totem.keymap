#define NAV 1
#define MOUSE 2
#define SYM 3
#define NUM 4
#define FUNC 5
#define MEDIA 6
#define GAME 7
#define GAMESWAP 8

// Mouse settings
#define MOVE_UP1 MOVE_VERT(-1400)
#define MOVE_DOWN1 MOVE_VERT(1400)
#define MOVE_LEFT1 MOVE_HOR(-1400)
#define MOVE_RIGHT1 MOVE_HOR(1400)
