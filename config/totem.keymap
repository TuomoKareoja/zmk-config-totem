//             ┃    XXX    ┃ BSPC LINE ┃ BSPC WORD ┃ DEL WORD  ┃ DEL LINE  ┃   ┃ PRINT SCR ┃   HOME    ┃    END    ┃    XXX    ┃  CLOSE W  ┃
//             ┣━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━┫   ┣━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━┫
//             ┃    LGUI   ┃   RALT    ┃   RCTRL   ┃   LSHFT   ┃    XXX    ┃   ┃   LEFT    ┃   DOWN    ┃    UP     ┃   RIGHT   ┃    APP    ┃
// ┏━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━┫   ┣━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━┓
// ┃    ESC    ┃    UNDO   ┃    CUT    ┃   COPY    ┃   PASTE   ┃    XXX    ┃   ┃    XXX    ┃  PG DOWN  ┃   PG UP   ┃    XXX    ┃    XXX    ┃    XXX    ┃
// ┗━━━━━━━━━━━┻━━━━━━━━━━━┻━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━┫   ┣━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━┻━━━━━━━━━━━┻━━━━━━━━━━━┛
