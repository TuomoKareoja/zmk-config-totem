&mmv {
    time-to-max-speed-ms = <300>;
    delay-ms = <1>;
};
&msc {
    time-to-max-speed-ms = <100>;
    delay-ms = <1>;
};

&lt {
    flavor = "hold-preferred";
    tapping-term-ms = <5000>;
    retro-tap;
    hold-while-undecided;
    quick-tap-ms = <0>;
};

/ {
     combos {
            compatible = "zmk,combos";
            combo_toggle_game {
                timeout-ms = <200>;
                layers = <0>;
                key-positions = <20 31>;
                bindings = <&to GAME>;
            };
            combo_toggle_base {
                timeout-ms = <200>;
                layers = <7>;
                key-positions = <20 31>;
                bindings = <&to BASE>;
            };
            combo_toggle_caps_word {
                timeout-ms = <100>;
                layers = <0>;
                key-positions = <13 16>;
                bindings = <&caps_word>;
            };
    };

    macros {
        undo: undo {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp RCTRL>
                , <&macro_tap &kp Z>
                , <&macro_release &kp RCTRL>
                ;
        };
        cut: cut {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp RCTRL>
                , <&macro_tap &kp X>
                , <&macro_release &kp RCTRL>
                ;
        };
        copy: copy {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp RCTRL>
                , <&macro_tap &kp C>
                , <&macro_release &kp RCTRL>
                ;
        };
        paste: paste {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp RCTRL>
                , <&macro_tap &kp V>
                , <&macro_release &kp RCTRL>
                ;
        };
        bspc_line: bspc_line {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp LSHFT>
                , <&macro_tap &kp HOME>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp DEL>
                ;
        };
        del_line: del_line {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp LSHFT>
                , <&macro_tap &kp END>
                , <&macro_release &kp LSHFT>
                , <&macro_tap &kp DEL>
                ;
        };
        bspc_word: bspc_word {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp RCTRL>
                , <&macro_tap &kp BSPC>
                , <&macro_release &kp RCTRL>
                ;
        };
        del_word: del_word {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp RCTRL>
                , <&macro_tap &kp DEL>
                , <&macro_release &kp RCTRL>
                ;
        };
        close_w: close_w {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp RALT>
                , <&macro_tap &kp F4>
                , <&macro_release &kp RALT>
                ;
        };
    };

    behaviors {

        hmr: homerow_mods_right {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "balanced";
            tapping-term-ms = <190>;
            quick-tap-ms = <190>;
            require-prior-idle-ms = <100>;
            hold-trigger-key-positions = <                     \
                    0   1   2   3   4                          \
                    10  11  12  13  14                         \
                20  21  22  23  24  25                         \
                                33  34                         \
            >;
            hold-trigger-on-release;
            bindings =
                <&kp>,
                <&kp>;
        };
        hml: homerow_mods_left {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "balanced";
            tapping-term-ms = <190>;
            quick-tap-ms = <0>;
            require-prior-idle-ms = <100>;
            hold-trigger-key-positions = <                     \
                                        5   6   7   8   9      \
                                        15  16  17  18  19     \
                                        26  27  28  29  30  31 \
                                        35  36  37             \
            >;
            hold-trigger-on-release;
            bindings =
                <&kp>,
                <&kp>;
        };
        hmli: homerow_left_index {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "balanced";
            retro-tap;
            tapping-term-ms = <150>;
            quick-tap-ms = <150>;
            require-prior-idle-ms = <70>;
            hold-trigger-key-positions = <                     \
                                        5   6   7   8   9      \
                                        15  16  17  18  19     \
                                        26  27  28  29  30  31 \
                                        35                     \
            >;
            hold-trigger-on-release;
            bindings =
                <&kp>,
                <&kp>;
        };
        hmri: homerow_right_index {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "balanced";
            retro-tap;
            tapping-term-ms = <150>;
            quick-tap-ms = <150>;
            require-prior-idle-ms = <70>;
            hold-trigger-key-positions = <                     \
                    0   1   2   3   4                          \
                    10  11  12  13  14                         \
                20  21  22  23  24  25                         \
                                    34                         \
            >;
            bindings =
                <&kp>,
                <&kp>;
        };
        scolon: shift_colon {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&kp COLON>, <&kp SEMI>;
            mods = <(MOD_LSFT|MOD_RSFT)>;
        };
        hmr_scolon: homerow_right_scolon {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "balanced";
            tapping-term-ms = <190>;
            quick-tap-ms = <190>;
            require-prior-idle-ms = <100>;
            hold-trigger-key-positions = <                     \
                    0   1   2   3   4                          \
                    10  11  12  13  14                         \
                20  21  22  23  24  25                         \
                                33  34                         \
            >;
            hold-trigger-on-release;
            bindings = <&kp>, <&scolon>;
        };
        sspace: shift_space {
            compatible = "zmk,behavior-mod-morph";
            #binding-cells = <0>;
            bindings = <&kp SPACE>, <&kp UNDER>;
            mods = <(MOD_LSFT|MOD_RSFT)>;
        };
        lt_sspace: lt_shift_space {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            tapping-term-ms = <5000>;
            retro-tap;
            hold-while-undecided;
            quick-tap-ms = <0>;
            bindings = <&kp>, <&sspace>;
        };

    };


        keymap {
                compatible = "zmk,keymap";
// ▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄

                base_layer {
// ┏━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
label= "BASE"                              ;
// ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
//             ┏━━━━━━━━━━━┳━━━━━━━━━━━┳━━━━━━━━━━━┳━━━━━━━━━━━┳━━━━━━━━━━━┓   ┏━━━━━━━━━━━┳━━━━━━━━━━━┳━━━━━━━━━━━┳━━━━━━━━━━━┳━━━━━━━━━━━┓
//             ┃     Q     ┃     W     ┃     E     ┃     R     ┃     T     ┃   ┃     Y     ┃     U     ┃     I     ┃     O     ┃     P     ┃
//             ┣━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━┫   ┣━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━┫
//             ┃     A     ┃     S     ┃     D     ┃     F     ┃     G     ┃   ┃     H     ┃     J     ┃     K     ┃     L     ┃     :     ┃
// ┏━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━┫   ┣━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━┓
// ┃    ESC    ┃     Z     ┃     X     ┃     C     ┃     V     ┃     B     ┃   ┃     N     ┃     M     ┃     ,     ┃     .     ┃     /     ┃     '     ┃
// ┗━━━━━━━━━━━┻━━━━━━━━━━━┻━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━┫   ┣━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━┻━━━━━━━━━━━┻━━━━━━━━━━━┛
//                                     ┃    ESC    ┃   SPACE   ┃    TAB    ┃   ┃   ENTER   ┃    BSPC   ┃    DEL    ┃
//                                     ┗━━━━━━━━━━━┻━━━━━━━━━━━┻━━━━━━━━━━━┛   ┗━━━━━━━━━━━┻━━━━━━━━━━━┻━━━━━━━━━━━┛
 bindings = <
// ╷           ╷           ╷           ╷           ╷           ╷           ╷   ╷           ╷           ╷           ╷           ╷           ╷           ╷

                &kp Q       &kp W       &kp E         &kp R         &kp T           &kp Y       &kp U         &kp I       &kp O        &kp P
                &hml LGUI A &hml RALT S &hml RCTRL D  &hmli LSHFT F &kp G           &kp H       &hmri RSHFT J &hmr RCTRL K &hmr RALT L &hmr_scolon RGUI N0
    &kp ESC     &kp Z       &kp X       &kp C         &kp V         &kp B           &kp N       &kp M         &kp COMMA   &kp DOT      &kp FSLH    &kp APOS
                                        &lt MEDIA ESC &lt_sspace NUM N0 &lt NAV TAB           &lt MOUSE RET &lt SYM BSPC &lt FUNC DEL
                        >;
                };

// ▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄▄

                nav_layer {
// ┏━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
